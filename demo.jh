application {
  config {
    baseName websocket,
    applicationType microservice,
    packageName com.yinhaos,
    authenticationType no,
    prodDatabaseType mysql,
    websocket spring-websocket,
    serverPort 9004
  }
}

entity User {
	Id Integer
    name String
    password String
    createTime ZonedDateTime
    updateTime ZonedDateTime
}

entity UserResource {
	id Integer
    userId Integer
    type Integer
    createTime ZonedDateTime
    updateTime ZonedDateTime
}

entity Member {
	id Integer
    createTime ZonedDateTime
    updateTime ZonedDateTime
}

entity Demo {
	id Integer
    name String
}

relationship OneToMany {
  User to UserResource
}
    