
application {
  config {
    baseName gateWay,
    applicationType gateway,
    packageName com.yinhaos,
    authenticationType jwt,
    prodDatabaseType no,
    cacheProvider redis,
    serverPort 9000
  }
  entities *
}

application {
  config {
    baseName user,
    applicationType microservice,
    packageName com.yinhaos,
    authenticationType no,
    prodDatabaseType mysql,
    serverPort 9001
  }
  entities User, UserResource
}

application {
  config {
    baseName admin,
    applicationType microservice,
    packageName com.yinhaos,
    authenticationType no,
    prodDatabaseType mysql,
    serverPort 9002
  }
  entities Member
}

application {
  config {
    baseName demo,
    applicationType microservice,
    packageName com.yinhaos,
    authenticationType no,
    prodDatabaseType mysql,
    serverPort 9003
  }
  entities Demo
